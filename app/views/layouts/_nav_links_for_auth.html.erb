<% if user_signed_in? %>
  <%= nav_link current_user.email, edit_user_registration_path %>
<% end %>
<% if user_signed_in? %>
  <%= nav_link 'Sign out', destroy_user_session_path, method: :delete %>
<% else %>
  <%= nav_link 'Sign in', new_user_session_path %>
<% end %>
<% if user_signed_in? && current_user.admin? %>
  <li class='nav-item'><%= link_to 'Users', users_path, class: 'nav-link' %></li>
<% end %>
